<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.class_ic.dao.HomeworkDAO">

<!-- 과제 게시판 메인카테고리 출력 : 2017.06.27 최은혜 -->
<select id="selectCate" parameterType="String" resultType="com.class_ic.vo.HomeworkDTO">
	select cateTitle from category where email=#{email} 
</select>

<!-- 카테고리별 조 등록 -->
<insert id="addCate" parameterType="com.class_ic.vo.HomeworkDTO">
	insert into groupcategory(email,classCode,cateCode,teamName) 
	values(email=#{email}, classCode=#{classCode},cateCode=#{cateCode},teamName=#{teamName});
</insert> 



</mapper>
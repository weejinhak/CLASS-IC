<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.class_ic.dao.StudentListDAO">
	
	<!-- 학적부 : 2017.06.24 최은혜 -->
	<select id="selectStudent" parameterType="String" resultType="com.class_ic.vo.MemberDTO">
	select *
	from member m join classmember c on m.email = c.email
	where c.classCode=#{classCode};
	</select>

</mapper>
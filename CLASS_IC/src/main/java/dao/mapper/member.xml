<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.class_ic.app.dao.memberdao">


	<insert id="attendanceMember" parameterType="com.class_ic.app.dto.memberdto">
		insert into socialapp.attendance values(#{m_qr})
	</insert>

	<select id="login" resultType="Integer">
		select count(*) from
		socialapp.member2 where m_email=#{id} and m_pw=#{pw}
	</select>

</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.class_ic.dao.AttendanceListDAO">



	<select id="attendanceSelect" parameterType="String" resultType="String">
		select classCode from classMember where email=#{email} and classCode=#{classCode}
	</select>


</mapper>